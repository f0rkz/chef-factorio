{
  "name": "<%= @game_settings['name'] %>",
  "description": "<%= @game_settings['description'] %>",
  "tags": <%= @game_settings['tags'] %>,

  "_comment_max_players": "Maximum number of players allowed, admins can join even a full server. 0 means unlimited.",
  "max_players": <%= @game_settings['max_players'] %>,

  "_comment_visibility": ["public: Game will be published on the official Factorio matching server",
                          "lan: Game will be broadcast on LAN"],
  "visibility":
  {
    "public": <%= @game_settings['visibility']['public'] %>,
    "lan": <%= @game_settings['visibility']['lan'] %>
  },

  "_comment_credentials": "Your factorio.com login credentials. Required for games with visibility public",
  "username": "<%= @game_settings['username'] %>",
  "password": "<%= @game_settings['password'] %>",

  "_comment_token": "Authentication token. May be used instead of 'password' above.",
  "token": "<%= @game_settings['token'] %>",

  "game_password": "<%= @game_settings['game_password'] %>",

  "_comment_require_user_verification": "When set to true, the server will only allow clients that have a valid Factorio.com account",
  "require_user_verification": <%= @game_settings['require_user_verification'] %>,

  "_comment_max_upload_in_kilobytes_per_second" : "optional, default value is 0. 0 means unlimited.",
  "max_upload_in_kilobytes_per_second": <%= @game_settings['max_upload_in_kilobytes_per_second'] %>,

  "_comment_minimum_latency_in_ticks": "optional one tick is 16ms in default speed, default value is 0. 0 means no minimum.",
  "minimum_latency_in_ticks": <%= @game_settings['minimum_latency_in_ticks'] %>,

  "_comment_ignore_player_limit_for_returning_players": "Players that played on this map already can join even when the max player limit was reached.",
  "ignore_player_limit_for_returning_players": <%= @game_settings['ignore_player_limit_for_returning_players'] %>,

  "_comment_allow_commands": "possible values are, true, false and admins-only",
  "allow_commands": "<%= @game_settings['allow_commands'] %>",

  "_comment_autosave_interval": "Autosave interval in minutes",
  "autosave_interval": <%= @game_settings['autosave_interval'] %>,

  "_comment_autosave_slots": "server autosave slots, it is cycled through when the server autosaves.",
  "autosave_slots": <%= @game_settings['autosave_slots'] %>,

  "_comment_afk_autokick_interval": "How many minutes until someone is kicked when doing nothing, 0 for never.",
  "afk_autokick_interval": <%= @game_settings['afk_autokick_interval'] %>,

  "_comment_auto_pause": "Whether should the server be paused when no players are present.",
  "auto_pause": <%= @game_settings['auto_pause'] %>,

  "only_admins_can_pause_the_game": <%= @game_settings['only_admins_can_pause_the_game'] %>,

  "_comment_autosave_only_on_server": "Whether autosaves should be saved only on server or also on all connected clients. Default is true.",
  "autosave_only_on_server": <%= @game_settings['autosave_only_on_server'] %>,

  "_comment_admins": "List of case insensitive usernames, that will be promoted immediately",
  "admins": <%= @game_settings['admins'] %>
}
